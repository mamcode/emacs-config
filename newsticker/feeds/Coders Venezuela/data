  <span class=\"keyword\">def</span> <span class=\"predefined-constant\">self</span>.<span class=\"function\">from_email</span>(email)
    <span class=\"constant\">PasswordReset</span>.new(<span class=\"constant\">User</span>.find_by_email(email))
  <span class=\"keyword\">end</span>
<span class=\"keyword\">end</span></pre></div>
